{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "no-empty": false,
    "object-literal-sort-keys": false,
    "max-classes-per-file": [true, 5, "exclude-class-expressions"],
    "no-console": [
      false
    ]
  }
}